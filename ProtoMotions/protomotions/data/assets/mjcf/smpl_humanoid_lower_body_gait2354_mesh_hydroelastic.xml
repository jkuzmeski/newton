          <geom type="mesh" mesh="l_fibula_mesh" euler="1.571 0 0" rgba="0.8 0.6 0.4 1" density="1234.567901"/>
          
          <body name="L_Ankle" pos="0.0000 0.0000 -0.3982">
            <joint name="L_Ankle_x" type="hinge" pos="0 0 0" axis="1 0 0" stiffness="150" damping="15" armature="0.02" range="-10.0000 10.0000"/>
            <joint name="L_Ankle_y" type="hinge" pos="0 0 0" axis="0 1 0" stiffness="150" damping="15" armature="0.02" range="-45.0000 20.0000"/>
