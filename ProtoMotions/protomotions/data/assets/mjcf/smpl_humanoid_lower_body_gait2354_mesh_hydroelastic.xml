    <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
    
    <body name="Pelvis" pos="-0.0018 -0.2233 0.0282">
      <freejoint name="Pelvis"/>
      <!-- Using meshes for collision (hydroelastic compatible) -->
      <geom type="mesh" mesh="pelvis_mesh" euler="1.571 0 0" rgba="0.8 0.6 0.4 1" density="1000"/>
      <geom type="mesh" mesh="sacrum_mesh" euler="1.571 0 0" rgba="0.8 0.6 0.4 1" density="1000"/>
      <geom type="mesh" mesh="l_pelvis_mesh" euler="1.571 0 0" rgba="0.8 0.6 0.4 1" density="1000"/>
      
      <body name="L_Hip" pos="-0.0066 0.0950 -0.0910">
        <joint name="L_Hip_x" type="hinge" pos="0 0 0" axis="1 0 0" stiffness="250" damping="25" armature="0.02" range="-20.0000 45.0000"/>
        <joint name="L_Hip_y" type="hinge" pos="0 0 0" axis="0 1 0" stiffness="250" damping="25" armature="0.02" range="-30.0000 120.0000"/>
        <joint name="L_Hip_z" type="hinge" pos="0 0 0" axis="0 0 1" stiffness="250" damping="25" armature="0.02" range="-45.0000 45.0000"/>
        <!-- Using mesh for collision -->
        <geom type="mesh" mesh="femur_l_mesh" euler="1.571 0 0" rgba="0.8 0.6 0.4 1" density="2040.816327"/>
        
        <body name="L_Knee" pos="-0.0067 0.0000 -0.3789">
