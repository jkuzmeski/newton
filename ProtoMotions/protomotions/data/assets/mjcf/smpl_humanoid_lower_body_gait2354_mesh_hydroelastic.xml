            <body name="L_Toe" pos="0.1233 0.0000 -0.0550">
              <joint name="L_Toe_x" type="hinge" pos="0 0 0" axis="1 0 0" stiffness="75" damping="8" armature="0.02" range="-10.0000 10.0000"/>
              <joint name="L_Toe_y" type="hinge" pos="0 0 0" axis="0 1 0" stiffness="75" damping="8" armature="0.02" range="-20.0000 20.0000"/>
              <joint name="L_Toe_z" type="hinge" pos="0 0 0" axis="0 0 1" stiffness="75" damping="8" armature="0.02" range="-5.0000 5.0000"/>
              <!-- Using mesh for collision -->
              <geom type="mesh" mesh="l_bofoot_mesh" euler="1.571 0 0" rgba="0.8 0.6 0.4 1" density="1000"/>
            </body>
          </body>
        </body>
      </body>
      
      <body name="R_Hip" pos="-0.0066 -0.0950 -0.0910">
        <joint name="R_Hip_x" type="hinge" pos="0 0 0" axis="1 0 0" stiffness="250" damping="25" armature="0.02" range="-20.0000 45.0000"/>
        <joint name="R_Hip_y" type="hinge" pos="0 0 0" axis="0 1 0" stiffness="250" damping="25" armature="0.02" range="-30.0000 120.0000"/>
        <joint name="R_Hip_z" type="hinge" pos="0 0 0" axis="0 0 1" stiffness="250" damping="25" armature="0.02" range="-15.0000 15.0000"/>
        <!-- Using mesh for collision -->
